  "dependencies": {
    "@astrojs/check": "0.5.5",
    "@astrojs/mdx": "2.1.1",
    "@astrojs/rss": "4.0.5",
    "@astrojs/sitemap": "3.1.1",
    "@astrojs/tailwind": "5.1.0",
    "@formkit/tempo": "0.0.13",
    "astro": "4.4.4",
    "daisyui": "4.7.2",
    "sharp": "0.33.2",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "@eslint/js": "8.56.0",
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@tailwindcss/typography": "0.5.10",
    "@types/eslint": "8.56.3",
    "@types/node": "20.11.20",
    "@typescript-eslint/eslint-plugin": "7.0.2",
    "@typescript-eslint/parser": "7.0.2",
